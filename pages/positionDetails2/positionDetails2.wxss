/* pages/positionDetails/positionDetails.wxss */
.body{
  min-height: 100vh
}
.footer{
  width: 750rpx;
  height: 110rpx;
  position: fixed;
  bottom: 0;
  background: #ffffff;
  line-height: 110rpx;
  vertical-align: middle;
padding:15rpx;
box-sizing:border-box;
}
.footer .footerimages{
  width: 50rpx;
  height: 50rpx;
  margin:5rpx 15rpx;
  position: relative;
  vertical-align: middle;
  padding-top: 10rpx
}
.footer .footerimages2{
  width: 50rpx;
  height: 50rpx;
  margin:0rpx 0rpx;
  position: relative;
  vertical-align: middle
}
.footer cover-view{
  display: inline-block
}
.btn{
  display: inline-block;
  height: 80rpx;
  border-radius: 40rpx;
  width: 254rpx;
  text-align: center;
  line-height: 80rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
}
.bg_img{
  width: 690rpx;
  height: 402rpx;
  margin-top: 20rpx;
  margin-left: 30rpx;
  position: absolute;
  z-index: 1
}
.title_main{
  position: relative;
  z-index: 20;
   width: 690rpx;
  height: 402rpx;
  top: 20rpx;
  margin-left: 30rpx;
}
.title_main_header image{
  width: 100rpx;
  height: 100rpx;
  vertical-align: top;
  margin-right: 30rpx;
}
.title_main_header{
  padding: 80rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box
}
.title_main_header>view{
  display: inline-block
}
.title_main_header .companyInfo .view1{
  font-size: 36rpx;
  color: #141414;
  line-height: 44rpx;
  font-weight: 800;
  width: 400rpx;
  display: inline-block;
overflow: hidden;
text-overflow: ellipsis;
word-wrap: break-word;
white-space: normal !important;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
}
.title_main_header .companyInfo .view2{
font-size: 26rpx;
color: #B4B4B4;
margin-top: 10rpx
}
.title_main_header .tosee{
font-size: 26rpx;
color: #F76B24;
float: right;
margin-top: 20rpx;
margin-right: 20rpx;
}
.title_main_footer{
  padding: 90rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
}
.title_main_footer .view1 .text1{
  float: left;
  font-size: 34rpx;
color: #141414;
font-weight: 800;
width:350rpx;
display: inline-block;
overflow: hidden; white-space: nowrap; text-overflow: ellipsis;
}
.title_main_footer .view1 .text2{
  float: right;
  font-size: 36rpx;
color: #F76B24;
}
.title_main_footer .view1 .text3{
  font-size: 24rpx;
}
.title_main_footer .view5 .text1,.title_main_footer .view6 .text1{
  font-size: 24rpx;
  background: #f0f0f0;
  color: #888888;
  display: inline-block;
  padding: 10rpx 20rpx;
  margin-right: 20rpx;
}
.title_main_footer .view5 ,.title_main_footer .view6 {
 margin-top: 10rpx;
}
.title_main_footer .view1{
  overflow: hidden;
  margin-bottom: 14rpx;
}
.title_main_footer .view2{
  font-size: 26rpx;
  color: #B4B4B4;
}
.title_main_footer .view2 text{
  display: inline-block;
  padding: 0 20rpx 0 0;
  line-height: 26rpx;
  margin-bottom: 20rpx;
}
.title_main_footer .view2 text:not(:first-child){
  border-left: 1px solid #B4B4B4;
   padding: 0 20rpx;
}
.title_main_footer .view3 text{
  display: inline-block;
  color: #FFFFFF;
  font-size: 20rpx;
  padding: 5rpx 15rpx;
  background: #F76B24;
  border-radius: 6rpx;
  margin-right: 10rpx
}
.title_main_footer .view3{
    margin-bottom: 50rpx;
    height: 50rpx;
    overflow: hidden
}
.title_main_footer .view4 {
  font-size: 26rpx;
  color: #c8c8c8;
  margin-top: 50rpx
}
.title_main_footer .view4 .text1{
  display: inline-block
}
.title_main_footer .view4 .text2{
  font-size: 26rpx;
  float: right

}
.title_main_footer .view4 .text2 image{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-bottom: 4rpx
}
.position_box .scroll-x{
  width: 640rpx;
  height: 828rpx;
  border-radius: 10px;
  background: #FFFFFF;
    display: inline-block;
    margin-right: 30rpx;
    box-sizing: border-box;
    padding: 30rpx 56rpx;
    vertical-align: top
}
.position_box .scroll-x:first-child{
   margin-left: 60rpx;
}
.position_box .scroll-x:last-child{
   margin-right: 60rpx;
}
.position_box{
  padding: 40rpx 0rpx 150rpx 0rpx;
  
}
.position_box .scroll-x .title{
    text-align: center;
    font-size: 36rpx;
    color: #000000;
    font-weight: 800;
    padding-bottom: 30rpx;
}
.position_box .main{
  display:block;
  white-space: pre-wrap;
  color: #323232;
  font-size: 26rpx;  
  line-height: 50rpx;
  height: 650rpx;
}
.chakan{
  color: #F76B24;
}
.gwbs{
  width: 400rpx;
  height: 360rpx;
  margin-left:70rpx; 
  margin-top: 80rpx;
}
.main .btn{
  border: 2rpx solid #F9DC55;
border-radius: 45rpx; 
font-size: 30rpx;
color: #F9DC55;
text-align: center;
height: 80rpx;
width: 400rpx;
  margin-left:70rpx; 
  margin-top: 80rpx;
}
.main .btn::before{
  content:"";
  width: 5px;
  height: 5px;
  display: block;
  border-radius: 2.5px;
  background: #F9DC55;
  position: absolute;
  top: 46%;
  right: 30rpx;
  color: #F9DC55;
}
.main .btn::after{
  content:"";
  width: 5px;
  height: 5px;
  display: block;
  border-radius: 2.5px;
  background: #F9DC55;
  position: absolute;
  top: 46%;
  left: 30rpx;
  color: #F9DC55;
}
#myMap{
  width: 528rpx;
  height: 500rpx;
}